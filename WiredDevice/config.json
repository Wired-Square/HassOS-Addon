{
  "version": "0.0.5",
  "name": "Wired Square Device CAN",
  "slug": "wired_square_device_can",
  "description": "This enables CAN interfaces on HassOS.",
  "url":"https://wiredsquare.com",
  "arch": ["armhf", "armv7", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "apparmor": false,
  "full_access": true,
  "host_network": true,
  "init": false,
  "ingress": false,
  "privileged": ["NET_ADMIN", "SYS_ADMIN"],
  "stage": "experimental",
  "options": {
    "bitrate": "50000",
    "restart-ms": "100",
    "tx_queue_length": "1000",
    "can0": "false",
    "can0_oscillator": "16000000",
    "can0_interrupt": "23",
    "can1": "false",
    "can1_oscillator": "16000000",
    "can1_interrupt": "25"
  },
  "schema": {
    "bitrate": "str",
    "restart-ms": "str",
    "tx_queue_length": "str",
    "can0": "bool",
    "can0_oscillator": "int",
    "can0_interrupt": "int",
    "can1": "bool",
    "can1_oscillator": "int",
    "can1_interrupt": "int"
  },
  "map": [
    "config:rw"
  ]
}
